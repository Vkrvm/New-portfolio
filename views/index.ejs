    <script>
        // Function to handle scrolling with different target positions
        function scrollToSection(navId, offset) {
            document.getElementById(navId).addEventListener('click', function (event) {
                event.preventDefault(); // Prevent default anchor click behavior

                // Get the target section
                var targetSection = document.querySelector(this.getAttribute('href'));

                // Calculate the position to scroll to (accounting for the navbar height and custom offset)
                var targetPosition = targetSection.offsetTop - offset;

                // Smooth scroll to the calculated position
                window.scrollTo({
                    top: targetPosition,
                    behavior: 'smooth'
                });
            });
        }

        // Assign different offsets for each section
        scrollToSection('navAbout', 0);   // About Me section offset
        scrollToSection('navSkills', 0);  // Skills section offset
        scrollToSection('navServices', 106);// Services section offset
        scrollToSection('navProjects', 106);// Projects section offset
        scrollToSection('navContact', 0); // Contact Me section offset
    </script>

